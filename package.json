{
  "name": "naivechain",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "node main.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "crypto-js": "^3.1.8",
    "express": "^4.11.2",
    "ws": "^1.1.5"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/crypto-js": "^3.1.37",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/ws": "^3.2.0",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "typescript": "^2.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "json",
      "lcov"
    ]
  }
}
